% my @productid = keys %{session->{'cart'}} if session->{'cart'};
% foreach my $key (@productid){
%	$cartinfo=$cartinfo+session->{'cart'}{$key};
% }
% if($cartinfo==0){
					<img src="/media/images/emptycart.png" border=0 title="Пожалуйста, заполните корзину" alt="Корзина пока пуста"></p>
					<p align=center>Корзина пока пуста :(</p>
% }else{
					<a href="/cart/"><img src="/media/images/fuelcart.png" border=0 title="Открыть корзину" alt="Переход в корзину"></a></p>
					<p align=center><a href="/cart/">Товаров в корзине</a> <span class="badge badge-info"><%= $cartinfo %></span></p>
% }
				<p align=center><br/>
					<a href="http://vk.com/nastartshop" target=_blank><img src="/media/images/socbut/vk20x20.gif" alt="Группа Вконтакте"></a>
					<a href="http://club.nastartshop.ru" target=_blank><img src="/media/images/socbut/hf20x20.gif" alt="Клуб Здоровая Семья"></a>
				</p>
			</div><div class="span4">
 				 <div align="right">
